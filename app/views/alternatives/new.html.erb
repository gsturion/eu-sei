<h1>Crie as respostas para a pergunta <%= @question.content %></h1>

<%= simple_form_for [@classroom, @question] do |f|%>

  <%= f.input :content %>
  <%= f.input :is_correct %>
  <%= f.button :submit, class: 'btn btn-primary' %>

<% end %>

<p>
  <%= link_to 'Sair', question_path(@question) %>
</p>
